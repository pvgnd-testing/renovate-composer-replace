{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "rangeStrategy": "auto",
  "lockFileMaintenance": {
    "enabled": true,
    "schedule": [
      "at any time"
    ]
  },
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "labels": [
    "renovate"
  ],
  "vulnerabilityAlerts": {
    "labels": [
      "security"
    ]
  }
}
